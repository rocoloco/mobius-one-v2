# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# Database URL (use Supabase connection string)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT].supabase.co:5432/postgres?sslmode=require

# Anthropic Claude API
ANTHROPIC_API_KEY=sk-ant-api03-your-key-here

# Salesforce Integration
SALESFORCE_CLIENT_ID=your-salesforce-client-id
SALESFORCE_CLIENT_SECRET=your-salesforce-client-secret
SALESFORCE_ACCESS_TOKEN=your-salesforce-token
SALESFORCE_INSTANCE_URL=https://your-instance.salesforce.com

# NetSuite Integration
NETSUITE_BASE_URL=https://your-account.suitetalk.api.netsuite.com
NETSUITE_ACCOUNT_ID=your-account-id
NETSUITE_CONSUMER_KEY=your-consumer-key
NETSUITE_CONSUMER_SECRET=your-consumer-secret
NETSUITE_TOKEN_ID=your-token-id
NETSUITE_TOKEN_SECRET=your-token-secret

# Email Service
SENDGRID_API_KEY=your-sendgrid-key
SENDGRID_FROM_EMAIL=notifications@mobiusone.com

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Feature Flags
ENABLE_AI_RECOMMENDATIONS=true
ENABLE_AUTO_EXECUTION=false
ENABLE_MOCK_DATA=true